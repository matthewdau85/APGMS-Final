{
  "name": "@apgms/shared",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./operations/risk/safe-delete": {
      "types": "./dist/operations/risk/safe-delete.d.ts",
      "default": "./dist/operations/risk/safe-delete.js"
    },
    "./db": {
      "types": "./dist/db.d.ts",
      "default": "./dist/db.js"
    },
    "./db.js": {
      "types": "./dist/db.d.ts",
      "default": "./dist/db.js"
    }
  },
  "scripts": {
    "clean-dist": "echo skipping clean-dist for now",
    "build": "tsc -p tsconfig.json",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "prisma:generate": "node ../scripts/ensure-linux.mjs && prisma generate --schema=../infra/prisma/schema.prisma",
    "prebuild": "prisma generate --schema=../infra/prisma/schema.prisma"
  },
  "dependencies": {
    "@prisma/client": "6.19.0",
    "argon2": "^0.41.1",
    "nats": "^2.29.3",
    "otplib": "^12.0.1",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@cyclonedx/cyclonedx-npm": "^4.1.2",
    "@types/jest": "^29.5.14",
    "@types/node": "^24.10.1",
    "prisma": "6.19.0",
    "typescript": "^5.6.3"
  }
}
