{
  "compilerOptions": {
    "target": "ES2023",
    "lib": ["ES2023"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "types": ["node"],
    "baseUrl": "../../",
    "paths": {
      "@apgms/shared": ["shared/src/index.ts"],
      "@apgms/shared/*": ["shared/src/*"],
      "@apgms/shared-au/*": ["shared/au/*"]
    }
  },
  "include": ["src", "test", "src/schemas", "types"]
}
